<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >

    <item android:id="@+android:id/background">
        <shape android:shape="rectangle" >
            <solid android:color="@color/extra_uploadProgressBar_background_progress_dialog" />

            <corners android:radius="@dimen/extra_uploadProgressBar_background_progress_dialog" />
        </shape>
    </item>
    <item android:id="@+android:id/secondaryProgress">
        <clip>
            <shape android:shape="rectangle" >
                <solid android:color="@color/extra_uploadProgressBar_secondaryProgress_progress_dialog" />

                <corners android:radius="@dimen/extra_uploadProgressBar_secondaryProgress_progress_dialog" />
            </shape>
        </clip>
    </item>
    <item android:id="@+android:id/progress">
        <clip>
            <shape android:shape="rectangle" >
                <solid android:color="@color/extra_uploadProgressBar_p_progress_dialog" />

                <corners android:radius="@dimen/extra_uploadProgressBar_p_progress_dialog" />
            </shape>
        </clip>
    </item>

</layer-list>